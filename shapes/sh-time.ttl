	sh:class time:ProperInterval ;
	sh:nodeKind sh:BlankNodeOrIRI ;
	sh:targetObjectsOf time:intervalAfter ;
	.

sh-time:intervalAfter-subjects-shape
	a sh:NodeShape ;
	sh:class time:ProperInterval ;
	sh:nodeKind sh:BlankNodeOrIRI ;
	sh:targetSubjectsOf time:intervalAfter ;
