	sh:class time:ProperInterval ;
	sh:nodeKind sh:BlankNodeOrIRI ;
	sh:targetObjectsOf time:intervalDuring ;
	.

sh-time:intervalDuring-subjects-shape
	a sh:NodeShape ;
	sh:class time:ProperInterval ;
	sh:nodeKind sh:BlankNodeOrIRI ;
	sh:targetSubjectsOf time:intervalDuring ;
